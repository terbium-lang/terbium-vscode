{
  "name": "terbium-vscode",
  "author": {
    "name": "jay3332",
    "url": "https://jay3332.pages.dev"
  },
  "publisher": "jay3332",
  "icon": "assets/icon.png",
  "displayName": "Terbium",
  "description": "Language support for Terbium",
  "repository": {
    "url": "https://github.com/terbium-lang/terbium-vscode"
  },
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "terbium",
      "aliases": ["Terbium", "terbium"],
      "extensions": [".tb", ".trb", ".terbium"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "terbium",
      "scopeName": "source.terbium",
      "path": "./syntaxes/terbium.tmLanguage.json"
    }]
  }
}
